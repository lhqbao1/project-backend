<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
        }
    </style>
</head>

<body>
    <table style="width:100%">
        <tr>
            <th>Email</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Address</th>
        </tr>
        <% for(var i=0; i < dataDisplay.length; i++) { %>
            <tr>
                <td>
                    <%= dataDisplay[i].email %>
                </td>
                <td>
                    <%= dataDisplay[i].firstName %>
                </td>
                <td>
                    <%= dataDisplay[i].lastName %>
                </td>
                <td>
                    <%= dataDisplay[i].address %>
                </td>
                <td>
                    <a style="text-decoration: none; padding: 3px; " href="/edit-crud?id=<%= dataDisplay[i].id %> " type="button">Edit</a>
                    <a style="text-decoration: none; padding: 3px; " href="/detele-crud?id=<%= dataDisplay[i].id %>" type="button">Delete</a>
                </td>

            </tr>
            <% } %>
    </table>
</body>

</html>